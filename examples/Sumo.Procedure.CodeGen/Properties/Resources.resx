  <data name="GetProcedureParametersSql" xml:space="preserve">
    <value>SELECT 
	SPECIFIC_SCHEMA as [Schema], 
	SPECIFIC_NAME as [Procedure], 
	RIGHT(PARAMETER_NAME, LEN(PARAMETER_NAME) - 1) as [Name], 
	DATA_TYPE as [DataType], 
	PARAMETER_MODE as [Direction], 
	ORDINAL_POSITION as [Order], 
	CHARACTER_MAXIMUM_LENGTH as [MaxLength], 
	CHARACTER_SET_NAME as [Encoding]
FROM INFORMATION_SCHEMA.PARAMETERS
where IS_RESULT = 'NO'
order by SPECIFIC_SCHEMA, SPECIFIC_NAME, ORDINAL_POSITION</value>
  </data>
  <data name="GetProceduresSql" xml:space="preserve">
    <value>SELECT [SPECIFIC_NAME] as [Name], [specific_schema] as [Schema]  FROM INFORMATION_SCHEMA.ROUTINES
where ROUTINE_TYPE != 'FUNCTION'
order by 2, 1</value>
  </data>
  <data name="GetSchemasSql" xml:space="preserve">
    <value>SELECT [schema_name] as [Name] FROM INFORMATION_SCHEMA.SCHEMATA
where 
	[schema_owner] = 'dbo'
	and [schema_name] not like 'db_%'
order by 1</value>
  </data>
  <data name="GetTablesSql" xml:space="preserve">
    <value>SELECT [table_name] as [Name], [table_schema] as [Schema]
FROM INFORMATION_SCHEMA.TABLES
where 
	[table_schema] != 'sys'
	and [table_type] = 'BASE TABLE'
order by 2, 1</value>
  </data>
