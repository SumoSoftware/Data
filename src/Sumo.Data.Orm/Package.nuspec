<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
    <metadata>
        <id>Sumo.Data.Orm</id>
        <version>4.2.2</version>
        <title>Sumo.Data - ORM Foundations</title>
        
        <authors>Mark Lauter</authors>
        <owners>Sumo Software Corporation</owners>
        <copyright>Copyright 2018 Sumo Software Corporation</copyright>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <licenseUrl>https://github.com/SumoSoftware/Data/blob/master/LICENSE</licenseUrl>
        <projectUrl>https://github.com/SumoSoftware/Data</projectUrl>
        <iconUrl>http://sumosoftware.com/media/nuget-sumo.png</iconUrl>
        
        <description>Sumo.Data.Orm is a light-weight, IoC capable ORM. Database specific support libraries are available. SQLite: Sumo.Data.Sqlite and Sumo.Data.Schema.Sqlite. Microsoft Sql Server: Sumo.Data.SqlServer and Sumo.Data.Schema.SqlServer.</description>
        <releaseNotes>Release 3 is a complete rewrite targeting .NetStandard 2.0.</releaseNotes>
        <tags>database db orm ioc sumo sql ado</tags>
        <repository type="git" url="https://github.com/SumoSoftware/Data.git" />
        <dependencies>
            <group targetFramework=".NETStandard2.0">
                <dependency id="NetStandard.Library" version="2.0.3" exclude="Build,Analyzers" />
                <dependency id="Sumo.Data" version="4.2.2-beta" exclude="Build,Analyzers" />
            </group>
        </dependencies>
    </metadata>
