<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
    <metadata>
        <id>Sumo.Retry</id>
        <version>4.0.1</version>
        <title>Sumo Software Retry Proxy</title>

        <authors>Sumo Software Corporation</authors>
        <owners>Sumo Software Corporation</owners>
        <copyright>Copyright 2018 Sumo Software Corporation</copyright>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <licenseUrl>https://github.com/SumoSoftware/Data/blob/master/LICENSE</licenseUrl>
        <projectUrl>https://github.com/SumoSoftware/Data</projectUrl>
        <iconUrl>http://sumosoftware.com/media/nugetsumo.png</iconUrl>

        <description>Proxy that allows methods from any Interface/Class combo to be wrapped in a retry loop.</description>
        <releaseNotes>Initial release</releaseNotes>
        <tags>DispatchProxy RetryProxy Retry</tags>
        <repository type="git" url="https://github.com/SumoSoftware/Data.git" />
        <dependencies>
            <group targetFramework=".NETStandard2.0">
                <dependency id="System.Reflection.DispatchProxy" version="4.5" exclude="Build,Analyzers" />
            </group>
        </dependencies>
    </metadata>
